{
  "name": "reckoning",
  "private": true,
  "version": "3.0.3",
  "scripts": {
    "generate-api-client": "openapi --input ./public/schema-v1.yaml --output ./app/frontend/frontend/api/client --request ./app/frontend/frontend/api/request.ts.template --useOptions --client axios --name ReckoningApiV1 && prettier app/frontend/frontend/api/client --write",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "lint": "yarn run lint:js && yarn run lint:prettier",
    "lint:js": "eslint --ext .js,.ts,.vue --fix app/frontend test/javascript",
    "lint:prettier": "prettier app/frontend --check",
    "lint:prettier:fix": "prettier app/frontend --write",
    "test:e2e": "cypress run --browser chrome --headless",
    "test:e2e:report": "generate-mochawesome-report --jsonDir test/reports/e2e --output test/reports/e2e/html --charts"
  },
  "dependencies": {
    "@headlessui/vue": "1.6.7",
    "@heroicons/vue": "^2.0.10",
    "axios": "0.27.2",
    "date-fns": "^2.29.2",
    "pinia": "^2.0.21",
    "pinia-plugin-persistedstate": "^2.1.1",
    "tailwindcss": "^3.1.8",
    "uuid": "^9.0.0",
    "vee-validate": "^4.6.7",
    "vue": "3.2.38",
    "vue-router": "4.1.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.14",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.7.16",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.0.2",
    "cypress": "^12.17.4",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.8",
    "cypress": "^10.7.0",
    "eslint": "^8.23.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vitest": "^0.0.8",
    "eslint-plugin-vue": "^9.5.1",
    "jsdom": "^20.0.0",
    "openapi-typescript-codegen": "0.23.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "sass": "1.54.8",
    "stylelint": "^14.12.1",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard": "^28.0.0",
    "typescript": "^5.2.2",
    "vite": "3.0.9",
    "vite-plugin-pwa": "0.12.6",
    "vite-plugin-ruby": "3.1.2",
    "vitest": "0.22.1",
    "vue-tsc": "^0.40.13",
    "workbox-build": "6.5.4",
    "workbox-window": "6.5.4"
  }
}
